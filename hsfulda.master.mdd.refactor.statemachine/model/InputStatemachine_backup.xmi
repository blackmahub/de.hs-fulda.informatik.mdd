      id="12"
      source="//@states.2/@region.0/@states.0"
      target="//@states.2/@region.0/@states.1"
      action="//@actions.2"/>
  <transitions
      id="1"
      source="//@initial"
      target="//@states.1"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="5"
      source="//@states.1"
      target="//@states.1"
      action="//@actions.2"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="10"
      source="//@states.2"
      target="//@states.0"
      action="//@actions.0"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="4"
      source="//@states.1"
      target="//@states.1"
      action="//@actions.1"/>
  <transitions
      id="2"
      source="//@states.2/@region.0/@initial"
      target="//@states.2/@region.0/@states.0"/>
  <transitions
      id="3"
      source="//@states.2/@region.1/@initial"
      target="//@states.2/@region.1/@states.0"/>
  <actions name="a1"/>
  <actions name="a2"/>
  <actions name="a3"/>
</sm:Statemachine>
