      id="15"
      source="//@states.2/@region.1/@states.1"
      target="//@states.2/@region.1/@states.1"
      action="//@actions.2"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="14"
      source="//@states.2/@region.1/@states.0"
      target="//@states.2/@region.1/@states.1"
      action="//@actions.2"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="8"
      source="//@states.0"
      target="//@states.0"
      action="//@actions.1"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="11"
      source="//@states.2"
      target="//@states.2"
      action="//@actions.1"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="6"
      source="//@states.1"
      target="//@states.2"
      action="//@actions.0"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="9"
      source="//@states.0"
      target="//@states.0"
      action="//@actions.2"/>
  <transitions
