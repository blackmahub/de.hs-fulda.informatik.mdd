    <region>
      <states
          outgoings="//@transitions.8"
          name="B1"
          super="//@states.2"/>
      <states
          outgoings="//@transitions.0"
          name="B2"
          super="//@states.2"/>
      <initial
          outgoings="//@transitions.13"
          id="2"/>
    </region>
    <region>
      <states
          outgoings="//@transitions.3"
          name="B3"
          super="//@states.2"/>
      <states
          outgoings="//@transitions.2"
          name="B4"
          super="//@states.2"/>
      <initial
          outgoings="//@transitions.14"
          id="3"/>
    </region>
  </states>
  <initial outgoings="//@transitions.9"
      id="1"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="13"
      source="//@states.2/@region.0/@states.1"
      target="//@states.2"
      action="//@actions.2"/>
  <transitions
      xsi:type="sm:LabeledTransition"
      id="7"
      source="//@states.0"
      target="//@states.1"
      action="//@actions.0"/>
  <transitions
