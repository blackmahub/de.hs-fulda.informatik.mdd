    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sm="http://hsfulda.master.mdd.emf.statemachine"
    xsi:schemaLocation="http://hsfulda.master.mdd.emf.statemachine Statemachine.ecore"
    name="InputStatemachine">
  <states outgoings="//@transitions.1 //@transitions.4 //@transitions.7"
      name="C"/>
  <states outgoings="//@transitions.12 //@transitions.10 //@transitions.6"
      name="A"/>
  <states xsi:type="sm:ComplexState"
      outgoings="//@transitions.11 //@transitions.5"
